
<div class="row">
    <div class="col-lg-12">
        <h3 class="page-header">상세 읽기 - @Model.NoteName</h3>
    </div>
</div>

<div class="row">
    <div class="col-lg-10">
        <div class="panel panel-default">
            <div class="panel-heading">
                <label>제목 : </label>@Model.NoteName
            </div>
            <div class="panel-body">
                @Model.NoteContents
            </div>
        </div>
        <div class="panel-footer text-right">
            <a class="btn btn-primary" asp-controller="Note" asp-action="Index">목록</a>
        </div>
    </div>
</div>